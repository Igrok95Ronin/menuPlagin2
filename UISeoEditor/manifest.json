{
  "manifest_version": 3,
  "name": "Ali JS",
  "version": "1.0",
  "host_permissions": [
    "http://localhost:8080/*",
    "https://nubify.ru/*"
  ],
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost:8080/sites/*/*/",
        "https://nubify.ru/sites/*/*/"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": [
        "http://localhost:8080/*",
        "https://nubify.ru/*"
      ]
    }
  ]
}

